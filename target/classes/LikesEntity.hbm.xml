<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.exemple.bdd.entity.LikesEntity" table="likes" schema="cours_jee_exemple2">
        <id name="idLike">
            <column name="idLike" sql-type="int(11)"/>
        </id>
        <property name="idUtilisateur">
            <column name="idUtilisateur" sql-type="int(11)"/>
        </property>
        <property name="idCommentaire">
            <column name="idCommentaire" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>